    "shopping": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "intolerances": {
      "type": "component",
      "repeatable": false,
      "component": "shared.intolerances",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "health": {
      "type": "component",
      "repeatable": false,
      "component": "shared.health"
    },
    "cooking_alt": {
      "type": "component",
      "repeatable": true,
      "component": "shared.cooking-alternative"
